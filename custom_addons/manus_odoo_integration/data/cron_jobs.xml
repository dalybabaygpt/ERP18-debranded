<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="manus_cron_check_api_status" model="ir.cron">
            <field name="name">Manus: Check API Status</field>
            <field name="model_id" ref="model_manus_connector"/>
            <field name="state">code</field>
            <field name="code">model.get_active_connector().test_connection()</field>
            <field name="interval_number">12</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
